<h1>Edit profile</h1>

<div class="container">
  <div class="row">
    <div class="col-xs-8 col-xs-offset-2">
      <%= simple_form_for(@user, url: account_path) do |f| %>
        <%= f.input :name %>
        <%= f.input :description %>



        <%= f.button :submit, class: "btn-primary" %>
      <% end %>
    </div>
  </div>
</div>
